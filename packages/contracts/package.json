        "@semaphore-protocol/proof": "workspace:packages/proof",
        "@typechain/ethers-v5": "^10.0.0",
        "@typechain/hardhat": "^6.0.0",
        "@types/chai": "^4.3.0",
        "@types/download": "^8.0.1",
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.12",
        "@types/rimraf": "^3.0.2",
