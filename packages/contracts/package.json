{
  "name": "@webb-tools/semaphore",
  "version": "0.0.1-3b",
  "description": "A library to help deploy Semaphore usage on typescript.",
  "license": "MIT",
  "main": "dist/index.node.js",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.node.js"
  },
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/",
    "build/typechain/",
    "contracts/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "repository": "https://github.com/webb-tools/semaphore-anchor.git",
  "homepage": "https://github.com/webb-tools/semaphore-anchor.git/tree/main/packages/contracts",
  "scripts": {
    "build:watch": "rollup -c rollup.config.ts -w --configPlugin typescript",
    "build": "rimraf -rf ./dist && rollup -c rollup.config.ts --configPlugin typescript",
    "prepublishOnly": "yarn build",
    "docs": "typedoc src/index.ts --out ../../docs/proof",
    "start": "hardhat node",
    "compile": "hardhat compile",
    "deploy:verifier": "hardhat deploy:verifier",
    "deploy:verifiers": "hardhat run scripts/deploy-verifiers.ts",
    "deploy:semaphore": "hardhat deploy:semaphore",
    "deploy:semaphore-voting": "hardhat deploy:semaphore-voting",
    "deploy:semaphore-whistleblowing": "hardhat deploy:semaphore-whistleblowing",
    "verify:contracts": "hardhat run scripts/verify-contracts.ts",
    "test": "hardhat test --no-compile",
    "test:report-gas": "REPORT_GAS=true hardhat test",
    "test:coverage": "hardhat coverage --no-compile",
    "typechain": "hardhat typechain",
    "lint": "solhint 'contracts/**/*.sol'"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.0",
    "@types/download": "^8.0.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.12",
    "@types/rimraf": "^3.0.2",
    "chai": "^4.3.5",
    "circomlib": "^2.0.2",
    "circomlibjs": "^0.0.8",
    "download": "^8.0.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.8",
    "ffjavascript": "^0.2.54",
    "hardhat": "^2.9.7",
    "hardhat-gas-reporter": "^1.0.8",
    "js-logger": "^1.6.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "snarkjs": "^0.4.13",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.21",
    "ts-node": "^10.4.0",
    "typechain": "^8.0.0",
    "typedoc": "^0.23.15",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@ethersproject/bytes": "^5.5.0",
    "@ethersproject/solidity": "^5.5.0",
    "@ethersproject/strings": "^5.5.0",
    "@openzeppelin/contracts": "4.7.3",
    "@webb-tools/sdk-core": "0.1.4-108",
    "@webb-tools/semaphore-group": "workspace:packages/group",
    "@webb-tools/semaphore-identity": "workspace:packages/identity",
    "@webb-tools/semaphore-proof": "workspace:packages/proof",
    "@webb-tools/utils": "^0.2.7",
    "circomlibjs": "0.0.8",
    "ethers": "^5.6.8",
    "ffjavascript": "^0.2.54",
    "snarkjs": "^0.4.13",
    "typescript": "^4.5.5"
  },
  "config": {
    "solidity": {
      "version": "0.8.6"
    },
    "paths": {
      "contracts": "./contracts",
      "circuit": "./circuit",
      "tests": "./test",
      "cache": "./cache",
      "snarkjs-templates": "./snarkjs-templates",
      "build": {
        "contracts": "./build/contracts",
        "typechain": "./build/typechain"
      }
    }
  }
}
