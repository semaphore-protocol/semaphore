        "typedoc": "^0.22.11"
    },
    "dependencies": {
        "@ethersproject/bignumber": "^5.5.0",
        "@ethersproject/random": "^5.5.1",
        "@ethersproject/sha2": "^5.6.1",
        "@ethersproject/strings": "^5.6.1",
        "circomlibjs": "0.0.8",
        "rollup": "^2.77.2",
        "typescript": "^4.5.5"
    }
