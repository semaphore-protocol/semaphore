{
    "name": "@semaphore-anchor/identity",
    "version": "0.0.1",
    "description": "A library to create Semaphore-anchor identities.",
    "license": "GPL-3.0-or-later",
    "author": "Webb Developers <drew@webb.tools>",
    "main": "dist/index.node.js",
    "exports": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.node.js"
    },
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/",
        "src/",
        "LICENSE",
        "README.md"
    ],
    "repository": "https://github.com/webb-tools/semaphore-anchor.git",
    "homepage": "https://github.com/webb-tools/semaphore-anchor.git/tree/main/packages/proof",
    "scripts": {
        "build:watch": "rollup -c rollup.config.ts -w --configPlugin typescript",
