    "main": "dist/index.node.js",
    "exports": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.node.js"
    },
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/",
        "src/",
        "LICENSE",
        "README.md"
    ],
