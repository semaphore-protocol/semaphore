{
  "name": "semaphore-anchor",
  "version": "0.0.1-2",
  "description": "A zero-knowledge protocol for anonymous signalling on a cross-chain setting.",
  "license": "MIT",
  "homepage": "https://github.com/webb-tools/semaphore-anchor.git#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webb-tools/semaphore-anchor.git.git"
  },
  "bugs": {
    "url": "https://github.com/webb-tools/semaphore-anchor.git/issues"
  },
  "private": true,
  "scripts": {
    "start": "hardhat node",
    "compile": "yarn compile:contracts && sh scripts/copy_typechain.sh && yarn build",
    "compile:contracts": "hardhat compile",
    "build": "yarn workspaces foreach run build",
    "download:snark-artifacts": "hardhat run scripts/download-snark-artifacts.ts",
    "deploy:all": "hardhat run scripts/deploy-all.ts",
    "deploy:cross-chain": "yarn deploy:all --network chainA && yarn deploy:all --network chainB",
    "deploy:verifier": "hardhat deploy:verifier",
    "deploy:semaphore": "hardhat deploy:semaphore",
    "deploy:semaphore-voting": "hardhat deploy:semaphore-voting",
    "deploy:semaphore-whistleblowing": "hardhat deploy:semaphore-whistleblowing",
    "test:unit-tests": "hardhat test ./test/unit_test/*",
    "test": "hardhat test",
    "test:semaphore": "hardhat test ./test/unit_test/Semaphore.ts",
    "test:cross-chain": "hardhat test ./test/functional_test/CrossChain.ts",
    "test:report-gas": "REPORT_GAS=true hardhat test",
    "test:coverage": "hardhat coverage",
    "typechain": "hardhat typechain",
    "lint": "yarn lint:sol && yarn lint:ts",
    "lint:ts": "eslint . --ext .js,.ts",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "prettier": "prettier -c .",
    "prettier:write": "prettier -w .",
    "commit": "cz",
    "precommit": "lint-staged"
  },
  "workspaces": [
    "packages/group",
    "packages/identity",
    "packages/proof",
    "packages/semaphore"
  ],
  "packageManager": "yarn@3.2.1",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-typescript": "^7.17.12",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.0",
    "@types/download": "^8.0.1",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.12",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-jest": "^27.4.6",
    "chai": "^4.3.5",
    "circomlib": "^2.0.2",
    "circomlibjs": "^0.0.8",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^14.3.2",
    "download": "^8.0.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.7.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.8",
    "hardhat": "^2.9.7",
    "hardhat-abi-exporter": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.8",
    "jest": "^27.4.1",
    "jest-config": "^27.4.7",
    "js-logger": "^1.6.1",
    "lint-staged": "^12.3.2",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "rimraf": "^3.0.2",
    "rollup": "^2.64.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "snarkjs": "^0.4.13",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.21",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typechain": "^8.0.0",
    "typescript": "^4.5.5"
  },
  "config": {
    "solidity": {
      "version": "0.8.5"
    },
    "paths": {
      "contracts": "./contracts",
      "circuit": "./circuit",
      "tests": "./test",
      "cache": "./cache",
      "snarkjs-templates": "./snarkjs-templates",
      "build": {
        "snark-artifacts": "./fixtures",
        "contracts": "./build/contracts",
        "typechain": "./build/typechain"
      }
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ethersproject/abi": "^5.6.4",
    "@ethersproject/bytes": "^5.6.1",
    "@ethersproject/providers": "^5.6.8",
    "@openzeppelin/contracts": "^4.4.2",
    "@webb-tools/sdk-core": "0.1.4-108",
    "@webb-tools/test-utils": "0.1.4-108",
    "@zk-kit/incremental-merkle-tree.sol": "^1.0.0",
    "ffjavascript": "^0.2.54",
    "lerna": "~4.0.0",
    "rollup": "^2.77.2"
  }
}
